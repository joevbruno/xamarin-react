{
  "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "comma-dangle": 0,
      "no-trailing-spaces": 0,
      "spaced-comment": 0,
      "max-len": [2, 110, 2, {"ignoreUrls": true}],
      "react/prefer-stateless-function": 0,
      "arrow-body-style": 0,
      "consistent-return": 0,
      "array-callback-return": 0,
      "prefer-template": 0,
      "no-unneeded-ternary": 0,
      "no-case-declarations": 0,
      "react/jsx-no-bind": 0
    },
    "extends": "airbnb",
    "ecmaFeatures": {
      "modules": true,
      "jsx": true,
      "experimentalObjectRestSpread": true
    },
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint"
}
