{
  "presets": ["es2015", "react", "stage-0"],
  "plugins": [
    "add-module-exports",
    "transform-decorators-legacy",
    "transform-runtime"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-member-expression-literals",
        "transform-property-literals",
        "transform-remove-debugger",
        "transform-remove-console"
      ]
    }
  }
}
